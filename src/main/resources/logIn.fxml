<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shoporswap.Controller">
    <children>
        <TextField fx:id="usernameTxtFld" layoutX="140.0" layoutY="40.0" promptText="username" />
        <TextField fx:id="passwordTxtFld" layoutX="140.0" layoutY="80.0" promptText="password" />
        <Button fx:id="createAccount" layoutX="39.0" layoutY="341.0" mnemonicParsing="false" onMouseClicked="#newAccount" text="Create New Account" />
        <Label fx:id="usernameLabel" layoutX="27.0" layoutY="38.0" prefHeight="27.0" prefWidth="105.0" text="Username" visible="true">
         <font>
            <Font size="20.0" />
         </font></Label>
        <Label fx:id="errorLabel" layoutX="323.0" layoutY="80.0" prefHeight="57.0" prefWidth="257.0" text="error: Incorrect username or password" visible="false" wrapText="true">
         <font>
            <Font size="20.0" />
         </font></Label>
        <Label fx:id="passwordLabel" layoutX="27.0" layoutY="80.0" prefHeight="27.0" prefWidth="93.0" text="Password">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="exitButton" layoutX="460.0" layoutY="341.0" mnemonicParsing="false" onMouseClicked="#exitClicked" prefHeight="27.0" prefWidth="93.0" text="Exit" />
      <Button fx:id="logInButton" layoutX="248.0" layoutY="130.0" mnemonicParsing="false" onAction="#logIn" text="Log In" />
      <Button fx:id="create" layoutX="247.0" layoutY="314.0" mnemonicParsing="false" onAction="#create" text="Create" visible="false" />
      <Label layoutX="52.0" layoutY="187.0" />
      <Label fx:id="usernameNewLabel" layoutX="21.0" layoutY="182.0" prefHeight="27.0" prefWidth="105.0" text="Username" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTxtFld1" layoutX="140.0" layoutY="182.0" visible="false" />
      <TextField fx:id="passwordTxtFld1" layoutX="140.0" layoutY="226.0" visible="false" />
      <Label fx:id="errorLabel1" layoutX="323.0" layoutY="218.0" prefHeight="57.0" prefWidth="257.0" text="error: Incorrect username or password" visible="false" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel1" layoutX="21.0" layoutY="226.0" prefHeight="27.0" prefWidth="93.0" text="Password" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
