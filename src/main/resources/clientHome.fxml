         </children>
      </VBox>
      <VBox layoutX="400.0" layoutY="100.0" prefHeight="300.0" prefWidth="200.0" style="-fx-border-color: #000000;">
         <children>
            <Label fx:id="clientHomeMyMessagesHeader" alignment="CENTER" prefWidth="200.0" text="My Messages">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Label>
            <ListView fx:id="clientHomeMyMessagesListView" prefHeight="278.0" prefWidth="198.0" style="-fx-border-color: #000000;" />
         </children>
      </VBox>
